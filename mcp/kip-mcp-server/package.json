{
  "name": "@ldclabs/kip-mcp-server",
  "mcpName": "io.github.ldclabs/KIP",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "bin": {
    "kip-mcp-server": "./dist/index.js"
  },
  "files": [
    "dist/",
    "docs/",
    "README.md",
    "package.json",
    "server.json"
  ],
  "scripts": {
